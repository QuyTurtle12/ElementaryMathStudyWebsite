@page
@model ElementaryMathStudyWebsite.RazorPage.Pages.CheckoutPages.AddItemsModel

@{
    ViewData["Title"] = "Add Items to Cart";
}

<h2>Add Items to Cart</h2>

<form method="post">
    <div>
        <label>Subject ID:</label>
        <input asp-for="CartItem.SubjectStudents" />
    </div>
    <div>
        <label>Child ID:</label>
        <input asp-for="CartItem" />
    </div>
    <button type="submit">Add to Cart</button>
</form>
